&GLOBAL
	PROJECT RUN
	RUN_TYPE ENEGY_FORCE
&END GLOBAL
&FORCE_EVAL
	METHOD Quickstep
	STRESS_TENSOR ANALYTICAL
	&DFT
		BASIS_SET_FILE_NAME BASIS_MOLOPT_SCAN
		POTENTIAL_FILE_NAME GTH_POTENTIALS_SCAN
		CHARGE 0
		MULTIPLICITY 1
		&MGRID
			CUTOFF 1200
			REL_CUTOFF 60
			NGRIDS 5
		&END MGRID
		&QS
			METHOD GPW
			EPS_DEFAULT 1e-14
			EXTRAPOLATION ASPC
		&END QS
		&POISSON
			PERIODIC XYZ
		&END POISSON
		&SCF
			SCF_GUESS RESTART
			MAX_SCF 50
			EPS_SCF 1e-07
			&OUTER_SCF
				EPS_SCF 1e-07
				MAX_SCF 10
			&END OUTER_SCF
			&OT
				PRECONDITIONER FULL_SINGLE_INVERSE
				MINIMIZER CG
			&END OT
		&END SCF
		&XC
			&XC_FUNCTIONAL
				&MGGA_X_R2SCAN
				&END MGGA_X_R2SCAN
				&MGGA_C_R2SCAN
				&END MGGA_C_R2SCAN
			&END XC_FUNCTIONAL
		&END XC
	&END DFT
	&SUBSYS
		&CELL
			A 12.6 0.0 0.0
			B 0.0 12.6 0.0
			C 0.0 0.0 12.6
		&END CELL
		&TOPOLOGY
			COORD_FILE_NAME ./run.xyz
			COORD_FILE_FORMAT XYZ
		&END TOPOLOGY
		&KIND H
			BASIS_SET TZV2P-MOLOPT-SCAN-GTH-q1
			POTENTIAL GTH-SCAN-1
		&END KIND H
		&KIND O
			BASIS_SET TZV2P-MOLOPT-SCAN-GTH-q6
			POTENTIAL GTH-SCAN-6
		&END KIND O
	&END SUBSYS
&END FORCE_EVAL
